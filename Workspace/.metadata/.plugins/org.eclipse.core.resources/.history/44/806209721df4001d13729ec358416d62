package monsters;

public class MonsterBaseClass {

	float movementSpeed;
	int attackPower;
	int goldSteal;
	Texture sprite;

	public MonsterBaseClass(float movementSpeed, int attackPower, int goldSteal) {
		super();
		this.movementSpeed = movementSpeed;
		this.attackPower = attackPower;
		this.goldSteal = goldSteal;
	}

}
