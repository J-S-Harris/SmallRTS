package monsters;

public class MonsterWeakSlow extends MonsterBaseClass {

	this.sprite = new Texture(Gdx.files.internal("puckDarkBlue.png"));
	
	public MonsterWeakSlow(float movementSpeed, int attackPower, int goldSteal) {
		super(movementSpeed, attackPower, goldSteal);
		// TODO Auto-generated constructor stub
	}

	public MonsterWeakSlow() {

		this.movementSpeed = 1;
		this.attackPower = 1;
		this.goldSteal = 5;
		
	}
}	
}
}
