package monsters;

import com.badlogic.gdx.Gdx;
import com.badlogic.gdx.graphics.Texture;

public class MonsterBaseClass {

	float movementSpeed;
	int attackPower;
	int goldSteal;
	Texture sprite;
	super.sprite=new Texture(Gdx.files.internal("puckDarkBlue.png"));

	public MonsterBaseClass(float movementSpeed, int attackPower, int goldSteal) {
		super();
		this.movementSpeed = movementSpeed;
		this.attackPower = attackPower;
		this.goldSteal = goldSteal;
	}
	
	public MonsterBaseClass() {
		// Use this to just set default movement speed;
	}

}
